
{
  "manifest_version":2,
  "name":"Three Day",
  "version":"1.0.1",
  "description":"保存三天中的浏览",
  "icons":{
    "16":"./img/spider_16.png",
    "32":"./img/spider_32.png",
    "128":"./img/spider_128.png"
  },
  "permissions":[
    "tabs",
    "nativeMessaging"
  ],
  "background":{
    "scripts":["background.js"]
  },
  "applications":{
    "gecko":{
      "id":"threeday@wingdust.com"
    }
  },
  "browser_action":{
    "default_icon":"./img/spider.png"
  }
}
