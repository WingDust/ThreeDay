
{
  "manifest_version":2,
  "name":"Three Day",
  "version":"1.0",
  "description":"保存三天中的浏览",
  "icons":{
    "16":"./img/icon.png",
    "32":"./img/icon.png",
    "128":"./img/icon.png"
  },
  "permissions":[
    "tabs",
    "nativeMessaging"
  ],
  "background":{
    "scripts":["background.js"]
  },
  "applications":{
    "gecko":{
      "id":"threeday@wingdust.com",
      "strict_min_version":"50.0"
    }
  },
  "browser_action":{
    "default_icon":"./img/icon.png"
  }
}
