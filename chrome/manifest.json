{
  "manifest_version":2,
  "name":"Three Day",
  "version":"1.0",
  "description":"保存三天中的浏览",
  "icons":{
    "16":"./img/spider.png",
    "32":"./img/spider.png",
    "128":"./img/spider.png"
  },
  "permissions":[
    "tabs",
    "nativeMessaging"
  ],
  "minimum_chrome_version":"23",
  "background":{
    "scripts":["background.js"],
    "persistent":false
  },
  "browser_action":{
    "default_icon":"./img/spider.png",
    "default_titile":"chrome"
  }
}
